<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1Navigation.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1Navigation"
    xmlns:pages="clr-namespace:MauiApp1Navigation.Pages"
    xmlns:profilePages="clr-namespace:MauiApp1Navigation.Pages.Profile"
    Title="MauiApp1Navigation"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate pages:JoinView}"
        Route="join" />
    
    <ShellContent
        ContentTemplate="{DataTemplate profilePages:IndexView}"
        Route="profile" />
    
    <!--<ShellContent
        ContentTemplate="{DataTemplate profilePages:AddressView}"
        Route="address" />-->

</Shell>
